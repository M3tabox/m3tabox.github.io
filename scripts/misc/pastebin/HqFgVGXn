FullFrame.Games.Template.Visible = false
for _, GameTable in pairs(GamesTable) do

	local GameV = FullFrame.Games:FindFirstChild("Template"):Clone()
	GameV.Parent = FullFrame.Games
	GameV.GameName.Text = GameTable.Name
	if PosToGo == 0 then
		PosToGo = 0.016
	else
		PosToGo = PosToGo + 0.06
	end
	GameV.BackgroundColor3 = GameTable.Colour
	GameV.Size = UDim2.new(0.932, 0, 0.05, 0)
	GameV.Position = UDim2.new(0.029,0,PosToGo,0)
	GameV.Visible = true
	GameV.Name = GameTable.Name
end

function figureNotifs(Stack,Container)

	local stacksize = 0

	local i = #Stack
	while i > 0 do
		local gui = Stack[i]
		if gui then
			if stacksize == 0 then
				stacksize = 0.05
			else
				stacksize = stacksize+0.075
			end
			local desiredpos = UDim2.new(0.5,0,stacksize,0)
			if gui.Position ~= desiredpos then
				gui:TweenPosition(desiredpos,"Out","Quint",0.3,true)
			end
		end
		i = i-1
	end
end

function pendNotifSmall(Content,Colour,Font)
	spawn(function()
		local notificationContainer = Container
		local Notification = notificationContainer:WaitForChild('Template')	
		local notifClone = Notification:Clone()
		local notifContent = notifClone:WaitForChild('Content')
		if Font == "GothamSemiBold" then
			Font = "GothamSemibold"
		end
		notifContent.BackgroundColor3 = Colour
		notifContent.Font = Font
		notifContent.TextTransparency = 1
		notifContent.BackgroundTransparency = 1
		notifClone.Name = 'Notif Clone'
		notifClone.Visible = true
		notifContent.Text = Content
		notifClone.Parent = Container
		notifClone.Position = UDim2.new(0.5,0,-0.1,0)
		wait(0.001)
		notifContent.Size = UDim2.new(0, math.max(18, notifContent.TextBounds.X * 1.2),1, 0)
		wait(0.5)
		notifClone:TweenPosition(UDim2.new(0.5,0,0.05,0),'Out','Quint',0.3,true)
		local transitionInfo = TweenInfo.new(0.35, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {BackgroundTransparency = 0})
		tween:Play()
		local transitionInfo = TweenInfo.new(0.35, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {TextTransparency = 0})
		tween:Play()
		table.insert(NotifStack,notifClone)
		figureNotifs(NotifStack,notificationContainer)


		wait(7)
		for a,b in pairs(NotifStack) do
			if b == notifClone then
				table.remove(NotifStack,a)
			end
		end
		wait(0.05)
		notifClone:TweenPosition(UDim2.new(0.5,0,notifClone.Position.Y.Scale+0.1,0),'Out','Quint',0.3,true)
		local transitionInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {BackgroundTransparency = 1})
		tween:Play()
		local transitionInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quint)
		local tween = game:GetService("TweenService"):Create(notifContent, transitionInfo, {TextTransparency = 1})
		tween:Play()
		wait(0.305)
		notifClone:Destroy()
		figureNotifs(NotifStack,notificationContainer)
	end)
end



local mt = getrawmetatable(game)
local old = mt.__namecall
local protect = newcclosure or protect_function

if not protect then
	protect = function(f) return f end
end

setreadonly(mt, false)
mt.__namecall = protect(function(self, ...)
	local method = getnamecallmethod()
	if method == "Kick" then
		pendNotifSmall("We stopped this game from kicking you locally",Color3.fromRGB(15, 107, 255),"GothamSemibold")
		wait(9e9)
		return
	end
	return old(self, ...)
end)

hookfunction(game.Players.LocalPlayer.Kick,protect(function() wait(9e9) end))

FullFrame.Size = UDim2.new(0.224, 0, 0.208, 0)
FullFrame.Parent = BarrierMain
FullFrame.Position = UDim2.new(0.376, 0, 1.2, 0)

for _, UI in ipairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
	if UI.Name == "BarrierMain" and UI ~= BarrierMain then
		if UI:FindFirstChild("FullFrame") then
			pendNotifSmall("Removing previous interface to declutter",Color3.fromRGB(85, 170, 127),"GothamSemibold")
			UI.FullFrame:TweenPosition(UDim2.new(0.376, 0, 0.626, 0), "Out", "Quint", 0.6)
			wait(0.61)
			UI.FullFrame:TweenSizeAndPosition(UDim2.new(0.224, 0, 0.208, 0),UDim2.new(0.376, 0, 1.2, 0),"In", "Quint", 0.8)	
			wait(0.81)
			UI:Destroy()
		end
	end
end

wait(0.5)
pendNotifSmall("Booting BarrierRemover v"..tostring(VersionNum)..".",Color3.fromRGB(0, 107, 156),"GothamSemibold")
wait(2)
pendNotifSmall("Booting client anti-kick",Color3.fromRGB(0, 107, 156),"GothamSemibold")
wait(1)
pendNotifSmall("WARNING! You could get banned using BarrierRemover, even without staff being in-game.",Color3.fromRGB(235, 67, 15),"GothamSemibold")
FullFrame:TweenSizeAndPosition(UDim2.new(0.247, 0, 0.256, 0),UDim2.new(0.376, 0, 0.626, 0),"Out", "Quint", 0.8)




function Work(Name)
	local num = 0

	if Name == "Frappé® V5" then --  Frappe V5

		if workspace:FindFirstChild("System") then
			if workspace.System:FindFirstChild("Barrier") then
				workspace.System["Barrier"]:Destroy()
				pendNotifSmall("Removed System Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
				num = num + 1
			end
			if workspace.System:FindFirstChild("Counter") then
				workspace.System.Counter:Destroy()
				pendNotifSmall("Removed Counter Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
				num = num + 1
			end
		end
		if workspace:FindFirstChild("Barista") then
			workspace.Barista:Destroy()
			pendNotifSmall("Removed Barista Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Premium") then
			workspace.Premium:Destroy()
			pendNotifSmall("Removed Premium Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Barista2") then
			workspace.Barista2:Destroy()
			pendNotifSmall("Removed Barista2 Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Staff") then
			workspace.Staff:Destroy()
			pendNotifSmall("Removed Staff Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
	elseif Name == "Frappé® V4" then --  Frappe V4
		if workspace:FindFirstChild("i") then
			workspace["i"]:Destroy()
			pendNotifSmall("Removed Staff Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("CounterRestriction") then
			workspace["CounterRestriction"]:Destroy()
			pendNotifSmall("Removed Barista Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Barrier") then
			workspace["Barrier"]:Destroy()
			pendNotifSmall("Removed Barista Barrier 2",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Inactivity") then
			workspace["Inactivity"]:Destroy()
			pendNotifSmall("Removed Barista Barrier 3",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Trainee") then
			workspace["Trainee"]:Destroy()
			pendNotifSmall("Removed Barista Barrier 4",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
	elseif Name == "Bakiez" then
		if workspace:FindFirstChild("Invis") then
			workspace["Invis"]:Destroy()
			pendNotifSmall("Removed Counter Barriers",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Functional") then
			if workspace.Functional:FindFirstChild("Doors") then
				workspace.Functional["Doors"]:Destroy()
				pendNotifSmall("Removed Kitchen Doors",Color3.fromRGB(25, 117, 255),"GothamSemibold")
				num = num + 1
			end
			if workspace.Functional:FindFirstChild("Workplace") then
				workspace.Functional["Workplace"]:Destroy()
				pendNotifSmall("Removed Counter Barriers 2",Color3.fromRGB(25, 117, 255),"GothamSemibold")
				num = num + 1
			end
			if workspace.Functional:FindFirstChild("VIPLounge") then
				workspace.Functional["VIPLounge"]:Destroy()
				pendNotifSmall("Removed VIPLounge Door",Color3.fromRGB(25, 117, 255),"GothamSemibold")
				num = num + 1
			end
		end
	elseif Name == "Fizze V1" then --  Fizze
		if workspace:FindFirstChild("StaffDoors") then
			workspace["StaffDoors"]:Destroy()
			pendNotifSmall("Removed Staff Barriers",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Barriers") then
			workspace["Barriers"]:Destroy()
			pendNotifSmall("Removed Main Barriers",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("BoatBarriers") then
			workspace["BoatBarriers"]:Destroy()
			pendNotifSmall("Removed Boat Barriers",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Floor") then
			workspace["Floor"]:Destroy()
			pendNotifSmall("Removed Floor detector",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
	elseif Name == "Burger King" then
		if workspace:FindFirstChild("kitchendoor") then
			workspace["kitchendoor"]:Destroy()
			pendNotifSmall("Removed Kitchen Door",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
	elseif Name == "Verde" then --  Verde
		for _, camerapart in ipairs(workspace.Camera:GetChildren()) do
			if camerapart.ClassName == "Part" and camerapart.Name == "Part" then
				camerapart:Destroy()
				pendNotifSmall("Removed Camera Barrier",Color3.fromRGB(25, 117, 255),"GothamSemibold")
				num = num + 1
			end
		end
		if workspace:FindFirstChild("BGMusicSound") then
			if workspace["BGMusicSound"].Volume ~= 0 then
				workspace["BGMusicSound"].Volume = 0
				pendNotifSmall("Muting horrible music",Color3.fromRGB(25, 117, 255),"GothamSemibold")
				num = num + 1
			end
		end
		if workspace:FindFirstChild("Map") then
			for _, PartUnderWS in ipairs(workspace.Map:GetChildren()) do
				for _, PartUnderPart in ipairs(PartUnderWS:GetChildren()) do
					if PartUnderPart.ClassName == "Script" or PartUnderPart == "TouchTransmitter" then
						PartUnderPart:Destroy()
						pendNotifSmall("Removed Fake Uniform Giver",Color3.fromRGB(25, 117, 255),"GothamSemibold")
						num = num + 1
					end
				end
			end
		end


		if game.Players.LocalPlayer.PlayerGui:FindFirstChild("MainUI") then
			if game.Players.LocalPlayer.PlayerGui.MainUI:FindFirstChild("MenuFrame") then
				if game.Players.LocalPlayer.PlayerGui.MainUI.MenuFrame:FindFirstChild("MusicClient") then
					game.Players.LocalPlayer.PlayerGui.MainUI.MenuFrame["MusicClient"]:Destroy()
					pendNotifSmall("Removed really sneaky anti fly",Color3.fromRGB(25, 117, 255),"GothamSemibold")
					num = num + 1
				end
			end
		end
		if game:GetService("ReplicatedStorage"):FindFirstChild("MusicEvent") then
			game:GetService("ReplicatedStorage")["MusicEvent"]:Destroy()
		end
		local nils = getnilinstances()

		for _, Ins in ipairs(nils) do
			if Ins.ClassName == "LocalScript" then
				Ins.Disabled = true
				Ins:Destroy()
				if NILINS2 == false then
					NILINS = true
				end
			end
		end
		if NILINS == true then
			NILINS2 = true
			NILINS = false
			pendNotifSmall("Removed nil instances (AntiExploits)",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if workspace:FindFirstChild("Doors") then
			for _, Door in ipairs(workspace["Doors"]:GetChildren()) do
				if Door.CanCollide == true then
					Door.CanCollide = false
					pendNotifSmall("CanCollide disabled for doors",Color3.fromRGB(25, 117, 255),"GothamSemibold")
					break
				end
			end
			for _, Door in ipairs(workspace["Doors"]:GetChildren()) do
				if Door.CanCollide == true then
					Door.CanCollide = false
				end
			end

			num = num + 1
		end
		if workspace:FindFirstChild("Gamepass Doors") then
			workspace["Gamepass Doors"]:Destroy()
			pendNotifSmall("Removed Gamepass Barriers",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
		if game.Players.LocalPlayer.Backpack:FindFirstChild("LocalDoors") then
			game.Players.LocalPlayer.Backpack:FindFirstChild("LocalDoors"):Destroy()
			pendNotifSmall("Removed LocalDoors Script",Color3.fromRGB(25, 117, 255),"GothamSemibold")
