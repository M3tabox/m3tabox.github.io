			pendNotifSmall("Removed Hidden Door",Color3.fromRGB(25, 117, 255),"GothamSemibold")
			num = num + 1
		end
	end
	if num ~= 0 then
		wait(2)
		if num == 1 then
			pendNotifSmall("Successfully removed 1 instance",Color3.fromRGB(0, 170, 127),"GothamSemibold")
		else
			pendNotifSmall("Successfully removed ".. tostring(num) .." instances",Color3.fromRGB(0, 170, 127),"GothamSemibold")
		end
		num = 0
	else
		pendNotifSmall("We couldn't find any instances to remove",Color3.fromRGB(244, 163, 0),"GothamSemibold")
		num = 0
	end
end



for _, GameVV in ipairs(FullFrame.Games:GetChildren()) do
	if GameVV.ClassName == "Frame" then
		GameVV.Select.MouseButton1Click:Connect(function()
			Work(GameVV.Select.Parent.Name)
		end)
	end
end


local UserInputService = game:GetService("UserInputService")

local gui = FullFrame
local draggui = Topbar

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

draggui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

draggui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

Exit.MouseButton1Click:Connect(function()
	pendNotifSmall("Quitting BarrierRemover.",Color3.fromRGB(15, 107, 255),"GothamSemibold")
	FullFrame:TweenPosition(UDim2.new(0.376, 0, 0.626, 0), "Out", "Quint", 0.6)
	wait(0.61)
	FullFrame:TweenSizeAndPosition(UDim2.new(0.224, 0, 0.208, 0),UDim2.new(0.376, 0, 1.2, 0),"In", "Quint", 0.8)	
end)
